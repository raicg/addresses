json.extract! zipcode, :id, :street, :number

json.city do |json|
  json.id zipcode.city.id
  json.name zipcode.city.name
  json.state do |json|
    json.id zipcode.city.state.id
    json.name zipcode.city.state.name
  end
end

json.neighborhood do |json|
  json.id zipcode.neighborhood.id
  json.name zipcode.neighborhood.name
end

